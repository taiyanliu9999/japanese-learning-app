{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/$1"
    },
    {
      "src": "/dict/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/dict/$1"
    },
    {
      "src": "/env.js",
      "dest": "/env.js"
    },
    {
      "src": "/status.html",
      "dest": "/status.html"
    },
    {
      "src": "/direct-test.html",
      "dest": "/direct-test.html"
    },
    {
      "src": "/debug.html",
      "dest": "/debug.html"
    },
    {
      "src": "/build-info.json",
      "dest": "/build-info.json"
    },
    {
      "src": "/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, OPTIONS"
      },
      "dest": "/index.html"
    }
  ]
} 